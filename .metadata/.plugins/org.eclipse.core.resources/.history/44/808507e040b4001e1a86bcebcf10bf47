package ch04_control;

public class Ex993_연습문제 {

	public static void main(String[] args) {
//		for(int i = 10; i > 0; i -= 2) {
//			System.out.println(i); // 10, 8, 6, 4, 2
//		}
		int n1 = 10;
		int n2 = 2;
		int result = 0;
		char operation = '-';
		
		switch (operation) {
		case '-': 
			result = n1 - n2;
			break;
		case '-': 
			result = n1 - n2;
			break;
		case '-': 
			result = n1 - n2;
			break;
		default:
			System.out.println("잘못된 연산자입니다.");
			
		
		
		
	}

}
