package ch04_control;

public class Ex994_연습문제 {

	public static void main(String[] args) {
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 3; j++) {
				if (i == 2) {
					continue;
				}
				if (j == 1) {
					break;
				}
				System.out.println("A"); //  바깥쪽 반복문에서 i가 2일 때 안쪽 반복문을 건너뛰고, 안쪽 반복문에서 j가 1일 때 바깥쪽 반복문으로 나가면서 "A"를 출력하는 동작을 수행합니다.
			}
		}
	}
}
