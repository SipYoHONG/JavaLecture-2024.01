package ch02_type;

public class Ex02_Primitive {

	public static void main(String[] args) {
		// boolean
		boolean a = true, b = false;
		
		/*
		 * 정수 계열
		 * 		1. byte: 	1 바이트, -128 ~ 127
		 * 		2. short: 	2 바이트, -32768 ~ 32767
		 * 		3. int:		4 바이트, -2147483648 ~ 2147483647
		 * 		4. long:	8 바이트, -2**63 ~ 2**63 - 1
		 */

	}

}
